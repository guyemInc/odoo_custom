<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- View Form-->
    <record id="label_form_view" model="ir.ui.view">
        <field name="name">label.form</field>
        <field name="model">label</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="name"></field>
                        <field name="description"></field>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
       <!-- Action-->
    <record model="ir.actions.act_window" id="action_label" >
        <field name="name">label</field>
        <field name="res_model">label</field>
        <field name="view_mode">tree,form,kanban,graph,pivot</field>        
    </record>
    <!-- Version menu item list "sans action pour le premier élément  "-->
    <menuitem
        id="label_menu_in_contacts"
        name="Label"
        action="action_label"
        parent="contacts.menu_contacts"
        sequence="5"/> 
    <!--My Dashboard Main Menu Access-->
    <menuitem
            id="label_menu_in_main_app"
            name="Label"
            action="action_label"
            parent="spreadsheet_dashboard.spreadsheet_dashboard_menu_root"
            sequence="3"/>
</odoo>